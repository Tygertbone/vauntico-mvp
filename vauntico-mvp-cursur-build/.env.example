# Vauntico Environment Configuration
# Copy this file to .env and fill in your actual values

# Google Analytics 4
VITE_GA4_ID=G-30N4CHF6JR

# Mixpanel Analytics
# Get your token from: https://mixpanel.com/project/YOUR_PROJECT/settings/project
VITE_MIXPANEL_TOKEN=your_mixpanel_token_here

# Stripe Payment Processing (Production)
VITE_STRIPE_PUBLIC_KEY=your_stripe_public_key_here

# API Configuration
VITE_API_URL=https://api.vauntico.com

# Feature Flags
VITE_ENABLE_CLI_ONBOARDING=true
VITE_ENABLE_CREATOR_PASS=true
VITE_ENABLE_REFERRALS=true
VITE_ENABLE_EMBEDS=true

# Debug Mode (Development Only)
VITE_DEBUG_MODE=false
