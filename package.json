{
  "name": "vauntico",
  "version": "1.0.0",
  "type": "module",
  "description": "Vauntico - AI-Powered Content Creation Platform",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
